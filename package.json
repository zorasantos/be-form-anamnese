{
  "name": "be-form-anamnese",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "start": "node dist/server.js",
    "start:dev": "tsx watch src/server.ts",
    "prisma:generate": "npx prisma generate --schema=./src/infra/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/infra/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/infra/database/prisma/schema.prisma",
    "build": "tsup src/server.ts",
    "test": "jest",
    "test:unit": "jest --config jest.unit.config.ts",
    "test:e2e": "cross-env NODE_ENV=test jest --config jest.e2e.config.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.13",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "pg": "^8.10.0",
    "prisma": "^4.13.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "4.13.0",
    "bcrypt": "^5.1.0",
    "chalk": "^5.2.0",
    "cli-table3": "^0.6.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fastify": "^4.15.0",
    "jsonwebtoken": "^9.0.0",
    "zod": "^3.21.4"
  }
}
